{
  "name": "@sermas/toolkit",
  "version": "1.0.0-alpha.19",
  "description": "Sermas Toolkit Web",
  "type": "commonjs",
  "main": "cjs/index.js",
  "exports": {
    "require": "./cjs/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sermas-organization/sermas-toolkit-web.git"
  },
  "scripts": {
    "build:tsc:cjs": "tsc --module commonjs --moduleResolution node --outDir dist/cjs && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "build:tsc:mjs": "tsc --module es2020 --outDir dist/esm && cp package.esm.json dist/esm/package.json",
    "build": "npm run clean && npm run build:tsc:cjs && npm run build:tsc:mjs && cp ./package.json ./svelte.config.mjs ./vite.config.ts README.md ./dist/",
    "build:watch": "npm run clean && tsup --watch",
    "clean": "rm -rf ./dist"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/audioworklet": "^0.0.50",
    "@types/three": "^0.155.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@lookingglass/webxr": "^0.4.0",
    "@lookingglass/webxr-polyfill": "^0.1.1",
    "@mediapipe/drawing_utils": "^0.3.1675466124",
    "@mediapipe/holistic": "^0.5.1675471629",
    "@mediapipe/tasks-audio": "^0.10.7",
    "@mediapipe/tasks-vision": "^0.10.7",
    "@ricky0123/vad-web": "^0.0.16",
    "@sermas/api-client": "^1.0.0-alpha.22",
    "@tensorflow-models/pose-detection": "^2.1.3",
    "@tensorflow/tfjs": "^4.10.0",
    "@tensorflow/tfjs-backend-wasm": "^4.12.0",
    "@vladmandic/human": "^3.1.2",
    "axios": "^1.6.0",
    "eventemitter2": "^6.4.9",
    "keycloak-js": "^22.0.5",
    "mqtt": "^5.3.4",
    "three": "^0.158.0",
    "uuid": "^9.0.1"
  }
}